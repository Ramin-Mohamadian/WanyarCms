@model IEnumerable<Wanyar.Core.DTOs.Article.GetArticleForSHowViewModel>

@foreach(var item in Model)
{

    <div id="right-col-text">
        <a href="/Home/ShowArticle/@item.articleId" id="right-col-text">
            @item.ArticleTitle
            <img id="img-left-col-1" style="max-width:50px;max-height:50px" src="/Picture/@item.ArticleImageName">
        </a>
    </div>
}